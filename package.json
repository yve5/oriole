{
  "name": "organe",
  "version": "1.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "lint": "eslint ./src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest --coverage",
    "coverage": "vitest run --coverage",
    "deploy": "gh-pages -d dist",
    "lib": "npm run lib:index && npm run lib:i18n && npm run lib:icons",
    "lib:index": "vite build --config vite.config.lib.js",
    "lib:i18n": "NAME=i18n vite build --config vite.config.lib.js",
    "lib:icons": "NAME=icons vite build --config vite.config.lib.js"
  },
  "peerDependencies": {
    "date-fns": "^3.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0"
  },
  "dependencies": {
    "bootstrap": "5.3.3",
    "canvas-confetti": "1.9.3",
    "clsx": "2.1.1",
    "react-router-dom": "6.27.0",
    "redux-mock-store": "1.5.4",
    "vite": "5.4.8",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "2.1.2"
  },
  "devDependencies": {
    "@testing-library/react": "16.0.1",
    "@types/react": "18.3.11",
    "@types/react-dom": "18.3.1",
    "@vitejs/plugin-react": "4.3.2",
    "@vitest/coverage-v8": "2.1.2",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "gh-pages": "6.2.0",
    "jsdom": "25.0.1",
    "rollup-plugin-peer-deps-external": "2.2.4",
    "sass": "1.69.5"
  },
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js"
    },
    "./i18n": {
      "import": "./lib/i18n.esm.js",
      "require": "./lib/i18n.cjs.js"
    },
    "./icons": {
      "import": "./lib/icons.esm.js",
      "require": "./lib/icons.cjs.js"
    },
    "./*": "./lib/*.js"
  }
}
